set number
set autoindent
set tabstop=4
set expandtab
set history=50
set hlsearch
set incsearch
set ruler
set showcmd

syntax on
set wrap

filetype plugin on
autocmd FileType php set omnifunc=phpcomplete#CompletePHP

set encoding=utf-8
set fileencodings=utf-8,ucs-bom,shift-jis,gb18030,gbk,gb2312,cp936,utf-16,big5,euc-jp,latin1

set laststatus=2
set showcmd

